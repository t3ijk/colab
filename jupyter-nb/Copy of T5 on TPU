0]"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"dtRfqm3Odgh1","executionInfo":{"status":"aborted","timestamp":1696907240111,"user_tz":-480,"elapsed":4,"user":{"displayName":"DING FENG","userId":"08114039084591127673"}}},"source":["evaluate(references, predictions)"],"execution_count":null,"outputs":[]}]}